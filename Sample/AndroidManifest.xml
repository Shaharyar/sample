<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sample"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/mm"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.meetingmatch.main.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
 
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.meetingmatch.main.CreateEvent">    
        </activity>
        
        <activity android:name="com.meetingmatch.main.Calendars" >
        </activity>
		<activity android:name="com.meetingmatch.main.Agenda" >
        </activity>
        <activity android:name="com.meetingmatch.main.DayView" >
        </activity>
        <activity android:name="com.meetingmatch.main.MonthView" >
        </activity>
      	<activity android:name="com.meetingmatch.main.YearView" >
        </activity>
        <activity android:name="com.meetingmatch.share.Share" 
            	  android:theme="@android:style/Theme.Holo.Light.Dialog">
        </activity>
        <activity android:name="com.meetingmatch.share.EditEvents" 
            	  android:theme="@android:style/Theme.Holo.Light.Dialog">
        </activity>
        <activity android:name="com.meetingmatch.share.ViewEvents" 
            	  android:theme="@android:style/Theme.Holo.Light.Dialog">
        </activity>
    </application>
	<uses-permission
  		android:name="android.permission.WRITE_CALENDAR">
   	</uses-permission>
   	<uses-permission
  		android:name="android.permission.READ_CALENDAR">
  	</uses-permission>
  	<uses-permission android:name="android.permission.READ_CONTACTS" >
  	</uses-permission>
  	<uses-permission android:name="android.permission.SEND_SMS"/>
</manifest>
